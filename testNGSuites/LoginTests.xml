<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="testUtils.Listeners"/>
	</listeners>
	
	<parameter name="username" value="randomUsername"/>
	<parameter name="password" value="randomPassword"/>

  <test thread-count="5" name="Login Positive Tests">
	  <classes>
		  <class name="testCases.TC4_LoginTestsPositive"/>
	  </classes>
  </test>
  
  <test thread-count="5" name="Login Negative Tests">
	  <classes>
		  <class name="testCases.TC5_LoginTestsNegative"/>
	  </classes>
  </test> 
  
</suite> <!-- Suite -->
